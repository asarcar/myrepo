# User Specific Changes

#------------------------------------------------------------
# Context Specific 
#------------------------------------------------------------
if [ -f $HOME/.env_custom/.bashrc_custom_user_specific ]; then
  source $HOME/.env_custom/.bashrc_custom_user_specific
fi

if [ -f $HOME/.env_custom/.bashrc_custom_work ]; then
  source $HOME/.env_custom/.bashrc_custom_work
fi

# Execute user specific .alias 
if [ -f $HOME/.env_custom/.alias_custom ]; then
  source $HOME/.env_custom/.alias_custom
fi

# Execute user specific .function 
if [ -f $HOME/.env_custom/.function_custom ]; then
  source $HOME/.env_custom/.function_custom
fi
